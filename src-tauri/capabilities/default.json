{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "shell:allow-open",
    "fs:default",
    "fs:allow-appdata-meta-recursive",
    "fs:allow-appconfig-write-recursive",
    "fs:allow-appdata-meta",
    "fs:allow-exists",
    {
      "identifier": "http:default",
      "allow": [
        "http://*"
      ]
    }
  ]
}